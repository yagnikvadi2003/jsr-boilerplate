    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "pre-commit": "^1.2.2",
    "prettier": "^3.0.3",
    "prettier-eslint": "^15.0.1",
    "sass": "^1.67.0",
    "sass-loader": "^13.3.2",
