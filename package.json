{
  "name": "boilerplate",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "main.tsx",
  "type": "commonjs",
  "scripts": {
    "start:web": "NODE_ENV=development webpack-dev-server --open --mode development --config ./webpack/webpack.config.js",
    "staging:web": "NODE_ENV=staging && webpack-dev-server --open --mode development --config ./webpack/webpack.config.js",
    "production:web": "NODE_ENV=testing webpack --mode production --config ./webpack/webpack.prod.js",
    "build:web": "NODE_ENV=production webpack --mode production --config ./webpack/webpack.prod.js",
    "build:staging:web": "NODE_ENV=staging webpack --mode production --config ./webpack/webpack.prod.js",
    "build:production:web": "NODE_ENV=production webpack --mode production --config ./webpack/webpack.prod.js",
    "lint": "eslint src/**/*.{js,ts,tsx,json}",
    "lint:fix": "eslint --fix 'src/**/*.{js,ts,tsx,json}'",
    "prettier": "prettier --write 'src/**/*.{js,ts,tsx,css,md,json}' --config ./.prettierrc",
    "pre:commit": "yarn run prettier && yarn run lint:fix",
    "commit-msg": "commitlint -e $GIT_PARAMS",
    "preview": "vite preview",
    "clean": "rm -rf build"
  },
  "lint-staged": {
    "*.{js,jsx}": "yarn run lint:fix",
    "**/*.{js,jsx,md,json}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yagnikvadi2003/your-repo/",
    "directory": "src"
  },
  "keywords": [
    "ReactJS",
    "Web Development",
    "react-router-dom version6",
    "bootstrap",
    "react-bootstrap",
    "react-icons",
    "Material UI"
  ],
  "author": {
    "name": "Yagnik Vadi",
    "email": "yagnikvadi8@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yagnikvadi2003/your-repo/issues"
  },
  "discussion": "https://github.com/yagnikvadi2003/your-repo/discussions",
  "homepage": "https://github.com/yagnikvadi2003/your-repo#readme",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "resolutions": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.20",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@commitlint/prompt-cli": "^17.7.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
